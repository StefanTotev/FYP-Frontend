{
  "manifest_version": 2,

  "name": "Sample Login",
  "description": "Sample login through backend",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery.js", "myScript.js"]
    }
  ],
  "permissions": [
    "http://localhost/*",
    "activeTab",
    "tabs",
    "https://ajax.googleapis.com/"
  ]
}